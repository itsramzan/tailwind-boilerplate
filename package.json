{
  "name": "tailwind-boilerplate",
  "version": "1.0.0",
  "description": "Tailwind CSS Boilerplate",
  "main": "index.js",
  "scripts": {
    "dev:tailwind": "npx tailwindcss -i ./src/input.css -o ./src/style.css --watch",
    "dev:server": "node ./node_modules/live-server/live-server.js --port=9000 ./src",
    "start": "concurrently \"yarn dev:tailwind\" \"yarn dev:server\""
  },
  "keywords": [],
  "author": "Ramzan Ali",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.1",
    "daisyui": "^3.8.1",
    "live-server": "^1.2.2",
    "tailwindcss": "^3.3.3"
  }
}
